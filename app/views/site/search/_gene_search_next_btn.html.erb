<div class="row">
  <div class="col-sm-offset-4 col-sm-4">
    <%= link_to "Load More Results (#{page} of #{pages})", '#', class: 'btn btn-lg btn-block btn-default', id: 'load-gene-search', data: {page: page} %>
  </div>
</div>

<script type="text/javascript" nonce="<%= content_security_policy_script_nonce %>">

    $('#load-gene-search').click(function(e) {
        e.preventDefault();
        $('#global-search-genes-form').submit();
    });

</script>