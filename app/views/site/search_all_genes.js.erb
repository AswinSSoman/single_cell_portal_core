<% @results.each do |gene| %>
    $("#gene-search-results").append("<%= escape_javascript(render partial: '/site/search/gene_result', locals: {gene: gene, study: @study, identifier: gene.id}) %>")
<% end %>
$('#gene-search-results').data('spinner').stop();
$('#gene-search-results-count').html($('.gene-panel').length);